<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <clip>
            <shape>
                <gradient android:startColor="#00ffffff" android:endColor="#00ffffff" android:angle="270.0" android:centerColor="#00ffffff" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff007a00" android:endColor="#ff007a00" android:centerColor="#ff007a00" />
            </shape>
        </clip>
    </item>
</layer-list>