<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:innerRadiusRatio="2.5" android:thicknessRatio="20.0" android:useLevel="false">
                <solid android:color="#ffd3d3cf" />
                <stroke android:width="1.0dip" android:color="#ffffffff" />
            </shape>
        </rotate>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:innerRadiusRatio="2.5" android:thicknessRatio="20.0" android:useLevel="true">
                <solid android:color="@color/stat_back_normal" />
                <stroke android:width="1.0dip" android:color="#ffffffff" />
            </shape>
        </rotate>
    </item>
</layer-list>